@charset "utf-8";

html,body {width:100%; height:100%;}
#dots-wrap {width:100%; height:100%; position: relative; display:flex;}

.dots-side-menu {width:320px; border-right:1px solid #EEEEEE; height:100%;}
#dsm .logo-wrap {height:51px; border-bottom:1px solid #EEEEEE;}
#dsm .logo-wrap .logo {display:inline-block; margin-top:15px; margin-left:30px;}
#dsm .search-box {padding:9px 10px;}
#dsm .search-box .search {display:inline-flex; background:#FBFBFB; width:100%; height:40px; border-radius:5px; overflow:hidden;}
#dsm .search-box .search button {border:none; outline:none; background:none; width:45px;}
#dsm .search-box .search button svg {stroke-width: 2; width:20px; height:20px;}
#dsm .search-box .search input {border:none; outline:none; background:none; font-size:14px; width:100%;}
#dsm .search-box .search input::placeholder {color:#AAAAAA;}

#dsm .fixed-menu {border-bottom:1px solid #EEEEEE; padding-bottom:12px;}
#dsm .fixed-menu ul {}
#dsm .fixed-menu ul li {height:45px; display:inline-flex; width:100%; align-items: center; justify-content: flex-start; padding-left:27px; cursor:pointer;}
#dsm .fixed-menu ul li span.ic {position:relative;}
#dsm .fixed-menu ul li span.ic svg {stroke-width: 1.5; width:20px; height:auto; color:#444444;}
#dsm .fixed-menu ul li span.text {margin-left:10px; color:#444444; font-size:15px; font-weight:500;}
#dsm .fixed-menu ul li span.ic em.indicator {position:absolute; right:-4px; top:-1px; background:#E10000; border-radius:50%; width:5px; height:5px;}

#dsm .variety-menu {border-bottom:1px solid #EEEEEE; padding-bottom:12px;}
#dsm .variety-menu ul {}
#dsm .variety-menu ul li {}
#dsm .variety-menu ul li .parent-menu {height:45px; display:inline-flex; width:100%; align-items: center; justify-content: flex-start; padding-left:27px; cursor:pointer; position:relative;}
#dsm .variety-menu ul li.has-depth .parent-menu::after {content:""; width:11px; height:6px; background:url("da-arrow.png") center center / contain no-repeat; position:absolute; right:18px; top:18px;}
#dsm .variety-menu ul li.has-depth .parent-menu-1 {height:45px; display:inline-flex; width:100%; align-items: center; justify-content: flex-start; padding-left:27px; cursor:pointer; position:relative;}
#dsm .variety-menu ul li.has-depth .parent-menu-2 {height:45px; display:inline-flex; width:100%; align-items: center; justify-content: flex-start; padding-left:27px; cursor:pointer; position:relative;}
#dsm .variety-menu ul li.has-depth .parent-menu-2::after {content:""; width:17px; height:29px; background:url("da-arrow-right.png") center center / contain no-repeat; position:absolute; right:18px; top:6px;}
#dsm .variety-menu ul li span.ic {position:relative;}
#dsm .variety-menu ul li span.ic svg {stroke-width: 1.5; width:20px; height:auto; color:#444444;}
#dsm .variety-menu ul li span.text {margin-left:10px; color:#444444; font-size:15px; font-weight:500;}
#dsm .variety-menu ul li span.ic em.indicator {position:absolute; right:-4px; top:-2px; background:#E10000; border-radius:50%; width:5px; height:5px;}
#dsm .variety-menu ul li .child-menu ul li {height:40px; display:inline-flex; width:100%; align-items: center; justify-content: flex-start; padding-left:27px; cursor:pointer; position:relative;}
#dsm .variety-menu ul li .child-menu ul li.active {background:rgb(42 85 255 / 2%);}
#dsm .variety-menu ul li .child-menu ul li.active::before {content:""; position:absolute; left:0; top:0; width:5px; height:100%; background:#2A55FF;}

#dsm .variety-menu ul li.active .parent-menu span.text.active {color:#2A55FF;}
#dsm .variety-menu ul li.active .parent-menu span.ic svg.active {color:#2A55FF;}

.person-container .person-con {display: flex; align-items: center; background: #FBFBFB; padding: 11px 12px 11px 15px; border-radius: 5px; width: 290px; height: 50px; margin-left: 15px; margin-right: 15px; margin-top: 280px; position: relative;}
.person-container .img {margin-right: 12px;}
.person-container .text {font-size: 15px; font-weight: 600; color: #444444;}
.person-container .text::after {content: ""; width: 10px; height: 10px; background: #93D955; border-radius: 50%; position: absolute; right: 15px; top: 20px;}
/* dots-contents */
.dots-contents {flex-grow: 1;
    display: flex;
    flex-direction: column;
    min-width: 0;
    min-height: 0;
    height: 100%;}

/* layers-wrap */
#layers-wrap {}
.layers-con {border-bottom:1px solid #EEEEEE; padding-bottom:15px;}
.layers-con ul {margin-left: 22px; }
.layers-con li {display: flex; margin-top: 16px;}
.layers-bold {font-size: 16px; font-weight: 700; color: #444444; margin-right: 50px; margin-left: 10px;}
.layers-menu {margin-right: 40px; color:#444444; font-size:14px; font-weight:500;}

.layers-menu.active span {color:#2A55FF; position: relative;}
.layers-menu.active span::after {content: ''; position: absolute; left: -80%; bottom: -19px; width: 66px; height: 3px; background: #2A55FF;}
.layers-menu.active span.ic svg {color:#2A55FF;}
/* layers-wrap */


/* list-wrap */
.list-contents {padding: 30px; position: relative; background: #F6F7F8;}
.list-contents .list-box {}
.list-contents .list-box .box {width: 100%; padding: 0 15px;}
.receipt-container { margin-bottom: 30px; }
.receipt ul {position: relative;}
.receipt ul li {margin-bottom: 80px;}
.receipt ul li:last-child {margin-bottom: 0;}
/* .receipt ul li .receipt-header {display: flex; align-items: center; width: 100%;} */

.receipt-head .head-name {display: inline-flex; align-items: center; flex: 1;}
.receipt-head .head-name .status {background: #256DCB; color: white; border-radius: 5px; font-size: 13px; font-weight: 500; padding: 5px 11px; display: flex; width: 67px; margin-left: 25px;}
.receipt-head .head-name .status span {color: white; font-size: 13px; font-weight: 500;}
.receipt-head .head-name .line {border: 1px solid #EEEEEE; width: 1402px; margin-right: 15px;}

.receipt-header {display: flex; margin-top: 15px; margin-bottom: 8px;}
.receipt-header .receipt-name .status {color: #BBBBBB; font-size: 12px; font-weight: 500; margin-left: 50px;}

.receipt-header .receipt-name {display: inline-flex; align-items: center; flex: 1;}
/* .receipt-header .receipt-name .status {background: #256DCB; color: white; border-radius: 5px; font-size: 13px; font-weight: 500; padding: 5px 11px; display: flex;} */
.receipt-header .receipt-field {display: inline-flex; margin-bottom: 8px;}
.receipt-header .receipt-field .rece-num span {color: #BBBBBB; font-size: 12px; font-weight: 500; }
.receipt-header .receipt-field .rece-col {text-align: center; width: 100px;}
.receipt-header .receipt-field .rece-col span {color: #BBBBBB; font-size: 12px; font-weight: 500;}

.receipt ul li .rece-content {overflow: hidden; border-radius: 5px; margin-left: 25px;}
.rece-content .rece-row {background: white; display: flex; align-items: center; height: 40px; margin-bottom: 8px;}
.rece-content .rece-row::before {content:""; width: 15px;
    height: 15px;
    background:url("da-blue.png") center center / contain no-repeat;    position:absolute; left:24px; top:29px;}

.rece-content .rece-row-2 {background: white; display: flex; align-items: center; height: 40px; margin-bottom: 8px;}
.rece-content .rece-row-2::before {content:""; width: 15px; height: 15px; position:absolute; left:24px; top:77px; background:url("da-blue.png") center center / contain no-repeat;}

.rece-content .rece-title {display: inline-flex; align-items: center; flex: 1; position: relative; padding-right:15px; padding-top:10px; padding-bottom:10px; min-width:300px;}
.rece-content .rece-name {position: relative;}
.rece-content .rece-name span {color: #666666; font-size: 15px; font-weight: 400; padding: 11px 0 0 25px;}
.rece-content .rece-list-field {display: inline-flex; align-items: center; margin-right: 100px;}
.rece-content .rece-list-field .rece-col {width: 100px; text-align: center; display: inline-flex; align-content: center; justify-content: center; align-items: center;}

.rece-content .rece-list-field .rece-col.company span {color: #666666; font-size: 15px; font-weight: 400; line-height: 22px;} 

.rece-content .rece-list-field .rece-col.date span {color: #666666; font-size: 15px; font-weight: 400; line-height: 22px;} 




